@model IEnumerable<Microsoft.AspNet.Identity.EntityFramework.IdentityRole>
@{
    ViewBag.Title = "AssignRoleToUser";
}

<h2>Assegna un Ruolo ad un Utente</h2>

@using (Html.BeginForm())
{

    <div class="form-group">
        @Html.Label("Seleziona il tipo di Utente", new { @class = "form-control" })
        <div class="col-md-10">
            @Html.DropDownList("UserName")
        </div>
    </div>
    <input type="submit" value="Crea Ruolo" />
}

